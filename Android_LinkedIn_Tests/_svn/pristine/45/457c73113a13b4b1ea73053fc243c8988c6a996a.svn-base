    <Description>/**
     * Taps on 'People Likes This' label.
     */
    public void tapOnPeopleLikeThisLabel() {
        for (TextView view : getSolo().getCurrentTextViews(null)) {
                String textOfView = view.getText().toString();
                if (textOfView.indexOf("people like this") &gt; -1 || textOfView.indexOf("person likes this") &gt; -1) {
                    ViewUtils.tapOnView(view, "'People Like This' label");
                    break;
                }
        }
    }</Description>
